"abbreviations
abbr h1 #
abbr h2 ##
abbr h3 ###
abbr h4 ####
abbr h5 #####

" Other Markdown stuff
nmap hd :.HeaderDecrease<CR>
nmap hi :.HeaderIncrease<CR>
nmap <leader>tf :TableFormat<CR>

noremap hd :.HeaderDecrease<CR>
noremap hi :.HeaderIncrease<CR>
noremap <leader>tf :TableFormat<CR>
set softtabstop=4
set conceallevel=2
